{
	"name": "@voxelio/converter",
	"version": "1.0.38",
	"type": "module",
	"description": "Handles transformation of datapacks into mod files for various platforms.",
	"scripts": {
		"build": "tsdown",
		"build:watch": "tsdown --watch",
		"check": "tsc --noEmit",
		"test": "vitest",
		"biome:format": "biome format --write ./src",
		"biome:check": "biome check ./src",
		"biome:unsafefix": "biome check --write --unsafe  ./src"
	},
	"dependencies": {
		"@voxelio/breeze": "workspace:*",
		"@voxelio/zip": "workspace:*"
	},
	"exports": {
		".": {
			"import": "./dist/index.mjs",
			"types": "./dist/index.d.mts"
		}
	},
	"files": [
		"dist/**/*.mjs",
		"dist/**/*.d.mts",
		"!dist/**/*.map"
	],
	"author": {
		"name": "Hardel",
		"email": "teams.voxel@gmail.com",
		"url": "https://voxel.hardel.io"
	},
	"publishConfig": {
		"access": "public"
	}
}
