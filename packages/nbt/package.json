{
    "name": "@voxelio/nbt",
    "version": "1.0.0",
    "type": "module",
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "types": "./dist/index.d.ts"
        }
    },
    "files": [
        "dist/**/*"
    ],
    "scripts": {
        "build": "tsdown",
        "build:watch": "tsdown --watch",
        "test": "vitest",
        "check": "tsc --noEmit",
        "biome:format": "biome format --write ./src",
        "biome:check": "biome check ./src",
        "biome:unsafefix": "biome check --write --unsafe  ./src"
    },
    "publishConfig": {
        "access": "public"
    }
}