{
    "compilerOptions": {
        "target": "ES2022",
        "module": "NodeNext",
        "moduleResolution": "NodeNext",
        "isolatedModules": true,
        "strict": true,
        "noImplicitAny": false,
        "esModuleInterop": true,
        "pretty": true,
        "composite": true,
        "sourceMap": true,
        "declaration": true,
        "declarationMap": true,
        "newLine": "LF",
        "skipLibCheck": true
    }
}